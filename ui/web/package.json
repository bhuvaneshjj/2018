{
  "name": "ui",
  "version": "2.1.1",
  "private": true,
  "---on react version upgrade---": "Update the version in .eslintrc as well.",
  "scripts": {
    "build": "react-app-rewired build",
    "clean": "rm -rf coverage",
    "clobber": "rm -rf coverage node_modules",
    "coverage": "react-app-rewired test --env=jsdom --coverage --colors",
    "lint": "eslint \"{../mobile/src/ui-common,src}/**/*.{js,jsx}\"",
    "lint-cache": "eslint --cache \"{../mobile/src/ui-common,src}/**/*.{js,jsx}\"",
    "prettier": "prettier --write \"{../mobile/src/ui-common,src}/**/*.{js,jsx,json,css}\"",
    "source-map-explorer": "npm run build && node ./node_modules/source-map-explorer/index.js build/static/js/*",
    "start": "react-app-rewired start",
    "test": "react-app-rewired test --env=jsdom --colors",
    "test:e2e": "npm run test:e2e:db && npm run test:e2e:puppeteer",
    "test:e2e:db": "echo ok",
    "test:e2e:puppeteer": "jest --colors --config src/end-to-end/jest.config.js",
    "test:watch": "winpty npm.cmd test"
  },
  "dependencies": {
    "bootstrap": "^3.4.1",
    "chnl": "^0.5.0",
    "dom-helpers": "^3.4.0",
    "jwt-decode": "^2.2.0",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0-alpha.0",
    "react-bootstrap": "^0.31.5",
    "react-bootstrap-typeahead": "^3.4.5",
    "react-debounce-input": "^3.2.0",
    "react-device-detect": "^1.6.2",
    "react-dnd": "^7.7.0",
    "react-dnd-html5-backend": "^7.7.0",
    "react-dom": "^16.9.0-alpha.0",
    "react-moment-proptypes": "^1.6.0",
    "react-redux": "^5.1.1",
    "react-responsive-modal": "^4.0.1",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^4.3.1",
    "react-style-proptype": "^3.2.2",
    "react-virtualized": "^9.21.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "sanitize-html": "^1.20.1",
    "websocket-as-promised": "^0.10.1"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "cross-env": "^5.2.0",
    "customize-cra": "^0.2.14",
    "deep-freeze": "0.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-react-native": "^3.7.0",
    "jest-environment-puppeteer": "^4.3.0",
    "jest-localstorage-mock": "^2.4.0",
    "jest-puppeteer": "^4.3.0",
    "jsonwebtoken": "^8.5.1",
    "mock-socket": "^8.0.5",
    "prettier": "^1.18.2",
    "puppeteer": "^1.18.1",
    "react-app-rewired": "^2.1.3",
    "react-dnd-test-backend": "^7.7.0",
    "react-mock-router": "^1.0.15",
    "react-scripts": "^3.0.1",
    "react-test-renderer": "^16.9.0-alpha.0",
    "redux-mock-store": "^1.5.3",
    "source-map-explorer": "^2.0.1"
  },
  "proxy": "http://localhost:4000",
  "browserslist": [
    "> 0.5%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "moduleNameMapper": {
      "ui-common/(.*)": "<rootDir>/../mobile/src/ui-common/$1"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/src/end-to-end"
    ],
    "roots": [
      "<rootDir>",
      "<rootDir>/../mobile/src/ui-common"
    ],
    "testMatch": [
      "**/*.test.{js,jsx,ts,tsx}"
    ]
  }
}