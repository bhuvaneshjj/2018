{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "build": "expo build:android && expo build:ios",
    "prettier": "prettier --write \"App.js\" \"src/**/*.{js,jsx,json,css}\"",
    "lint": "eslint App.js \"src/**/*.{js,jsx}\"",
    "lint-cache": "eslint --cache App.js \"src/**/*.{js,jsx}\"",
    "test": "node ./node_modules/jest/bin/jest.js --watchAll",
    "test:bash": "winpty npm.cmd run test",
    "test:ci": "jest --ci",
    "coverage": "jest --coverage",
    "clean": "rm -rf coverage",
    "clobber": "rm -rf coverage node_modules"
  },
  "dependencies": {
    "@expo/samples": "2.1.1",
    "chnl": "^0.4.2",
    "expo": "^32.0.0",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "16.5.0",
    "react-moment-proptypes": "^1.6.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-native-swiper": "^1.5.14",
    "react-style-proptype": "^3.2.2",
    "websocket-as-promised": "^0.9.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-preset-expo": "^5.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "eslint-plugin-react-native": "^3.7.0",
    "jest": "^24.7.1",
    "jest-expo": "^32.0.0",
    "mock-socket": "^8.0.5",
    "prettier": "^1.17.0",
    "react-test-renderer": "^16.8.6"
  },
  "private": true,
  "jest": {
    "coverageDirectory": "./coverage/",
    "preset": "jest-expo",
    "testPathIgnorePatterns": [
      "node_modules/"
    ]
  }
}
