{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "prettier": "prettier --write \"App.js\" \"src/**/*.{js,jsx,json,css}\"",
    "lint": "eslint --config ../../.eslintrc App.js \"src/**/*.{js,jsx}\"",
    "lint-cache": "eslint --config ../../.eslintrc --cache App.js \"src/**/*.{js,jsx}\"",
    "test": "jest --watchAll",
    "test:bash": "winpty npm.cmd run test"
  },
  "dependencies": {
    "@expo/samples": "2.1.1",
    "expo": "^32.0.0",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-navigation": "^3.0.9"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-preset-expo": "^5.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.7.1",
    "jest-expo": "^32.0.0",
    "prettier": "^1.17.0",
    "react-test-renderer": "^16.8.6"
  },
  "private": true,
  "jest": {
    "coverageDirectory": "./coverage/",
    "preset": "jest-expo",
    "testPathIgnorePatterns": [
      "node_modules/"
    ]
  }
}
